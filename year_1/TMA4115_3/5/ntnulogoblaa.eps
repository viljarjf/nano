%: NTNU-logo: Bredden er 1, pr. def.
%: Nedre venstre hjørne i (0,0)
%: Øvre høyre hjørne i (1,1)

/NTNUlogodict 25 dict def
NTNUlogodict begin

%: Definisjoner til innvortes bruk

/radius 2 sqrt 1 sub 2 div def

%: Definisjoner til utvortes bruk

/ytrekvadrat {
  .25 0 moveto
  .75 0 lineto
  .75 .25 .25 -90 0 arc
  1 .75 lineto
  .75 .75 .25 0 90 arc
  .25 1 lineto
  .25 .75 .25 90 180 arc
  0 .25 lineto
  .25 .25 .25 180 270 arc
  closepath } bind def

/indrekvadrat { %% Negativ omløpsretning!
  .25 .25 moveto
  .25 .75 lineto
  .75 .75 lineto
  .75 .25 lineto
  closepath } bind def

/sirkel {
  .5 radius add .5 moveto
  .5 .5 radius   0 180 arc
  .5 .5 radius 180 360 arc
  closepath } bind def

/mellom {indrekvadrat sirkel} def
/ytre   {ytrekvadrat indrekvadrat} def
/logo   {ytrekvadrat indrekvadrat sirkel} def

end
/NTNUlogo {NTNUlogodict begin exec end} bind def
%%EndResource

%%EndProlog
100 dup scale
{1.0 .75 .0 .05 setcmykcolor
 logo fill
} NTNUlogo

